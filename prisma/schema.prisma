generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model list_menu {
    id        Int      @id @default(autoincrement())
    icons     String   @db.VarChar(255)
    Name      String?
    created_at DateTime 
    updated_at DateTime 
}

model img_slide {
  id Int @id @default(autoincrement())
  img_url String?
  name String?
  link String?
  created_at DateTime 
  updated_at DateTime 
}